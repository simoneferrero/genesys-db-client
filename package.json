{
  "name": "genesys-sotb-client",
  "description": "The client-side application to manage Genesys - Shadow of the Beanstalk GM data",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "connected-react-router": "^6.3.2",
    "dotenv": "^7.0.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "styled-components": "^4.2.0",
    "urijs": "^1.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ignore-path .gitignore",
    "prettify": "prettier \"src/**/*.js\"",
    "format": "npm run prettify -- --write",
    "validate": "npm run lint && npm run prettify -- --list-different",
    "test:coverage": "npm run test -- --coverage",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.js",
      "!src/setupTests.js",
      "!src/mocks/*.js",
      "!src/serviceWorker.js",
      "!src/styles/*.js",
      "!src/**/*.stories.js",
      "!src/types/*.js",
      "!src/utils/definitions.js",
      "!src/utils/customTestRenderers.js",
      "src/**/*.{js|jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "lines": 100,
        "functions": 100
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-storyshots": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest-dom": "^3.1.3",
    "jest-specific-snapshot": "^2.0.0",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.16.4",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.8.5",
    "react-testing-library": "^6.0.3",
    "require-context.macro": "^1.0.4",
    "storybook": "^1.0.0"
  }
}
